 age =int(input("enter the your age : "))
height=int(input("enter your height in cm : "))
if age>12 and height>=120:
     print(" you can ride")
elif(age>12 and age<=18)and(height>110):
     print(" you can ride")
        
elif(age>18):
         print(" you can ride")
            
else:
     print(" you can not ride")
